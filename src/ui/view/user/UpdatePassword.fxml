<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.view.user.UpdatePasswordController">
   <children>
      <Label layoutX="196.0" layoutY="29.0" text="修改密码">
         <font>
            <Font size="27.0" />
         </font>
      </Label>
      <GridPane layoutX="85.0" layoutY="100.0" prefHeight="174.0" prefWidth="353.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="旧密码：" GridPane.rowIndex="1" />
            <Label text="新密码：" GridPane.rowIndex="2" />
            <Label text="确认新密码：" GridPane.rowIndex="3" />
            <PasswordField fx:id="oldpasswordTextField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <PasswordField fx:id="newpasswordTextField1" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <PasswordField fx:id="newpasswordTextField2" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <Label text="账号：" />
            <PasswordField fx:id="usernameTextField" GridPane.columnIndex="1" />
         </children>
      </GridPane>
      <Button layoutX="85.0" layoutY="311.0" mnemonicParsing="false" onAction="#back" prefHeight="40.0" prefWidth="92.0" text="取消" />
      <Button layoutX="346.0" layoutY="311.0" mnemonicParsing="false" onAction="#changepassword" prefHeight="40.0" prefWidth="92.0" text="确认" />
   </children>
</AnchorPane>
